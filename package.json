{
  "name": "guias-hoteles",
  "version": "1.0.0",
  "description": "nuestro primer proyecto de nodejs",
  "main": "index.html",
  "scripts": {
    "dev": "lite-server",
    "start": "concurrently \"npm run watch:scss\" \"npm run dev \"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange 'css/+.scss' -- npm run scss",
    "clean": "rimraf dist",
    "imagemin": "imagemin image/*--out-dir dist/image",
    "usemin": "usemin index.html -d dist --htmlmin -o dist/index.html && usemin about.html -d dist --htmlmin -o dist/about.html && usemin prices.html -d dist --htmlmin -o dist/prices.html && usemin contacto.html -d dist --htmlmin -o dist/contacto.html && usemin term-cond.html -d dist --htmlmin -o dist/term-cond.html",
    "build": "npm run clean && npm run scss && npm run imagemin && npm run usemin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://jhon0121@bitbucket.org/jhon0121/guias-hoteles.git"
  },
  "author": "john esteban",
  "license": "ISC",
  "homepage": "https://bitbucket.org/jhon0121/guias-hoteles#readme",
  "devDependencies": {
    "copyfiles": "^2.3.0",
    "cssmin": "^0.4.3",
    "grunt": "^1.3.0",
    "grunt-browser-sync": "^2.2.0",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^3.0.0",
    "grunt-contrib-imagemin": "^4.0.0",
    "grunt-contrib-sass": "^2.0.0",
    "grunt-contrib-uglify": "^5.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-filerev": "^2.3.1",
    "grunt-usemin": "^3.1.1",
    "htmlmin": "0.0.7",
    "imagemin": "^7.0.1",
    "imagemin-cli": "^6.0.0",
    "jit-grunt": "^0.10.0",
    "lite-server": "^2.5.4",
    "onchange": "^7.0.2",
    "rimraf": "^3.0.2",
    "time-grunt": "^2.0.0",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.6.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.5.2",
    "bootstrap": "^4.5.2",
    "bootstrap-icons": "^1.0.0",
    "concurrently": "^5.3.0",
    "jquery": "^3.5.1",
    "node-sass": "^4.14.1",
    "open-iconic": "^1.1.1",
    "popper.js": "^1.16.1",
    "tippy.js": "^6.2.6"
  }
}
